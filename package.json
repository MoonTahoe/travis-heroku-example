{
  "name": "travis-heroku-example",
  "version": "0.0.0",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prestart": "npm run build",
